title = {{Joint analysis of functional genomic data and genome-wide association studies of 18 human traits.}},
journal = {American journal of human genetics},
year = {2014},
volume = {94},
number = {4},
pages = {559--573},
month = apr,
affiliation = {New York Genome Center, New York, NY 10013, USA; Department of Biological Sciences, Columbia University, New York, NY 10027, USA. Electronic address: jkpickrell@nygenome.org.},
