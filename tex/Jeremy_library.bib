@article{Field:2016ky,
author = {Field, Yair and Boyle, Evan A and Telis, Natalie and Gao, Ziyue and Gaulton, Kyle J and Golan, David and Yengo, Lo{\"\i}c and Rocheleau, Ghislain and Froguel, Philippe and McCarthy, Mark I and Pritchard, Jonathan K},
title = {{Detection of human adaptation during the past 2000 years.}},
journal = {Science},
year = {2016},
volume = {354},
number = {6313},
pages = {760--764},
month = nov,
affiliation = {Department of Genetics, Stanford University, Stanford, CA 94305, USA. yairf@stanford.edu pritch@stanford.edu.},
doi = {10.1126/science.aag0776},
pmid = {27738015},
pmcid = {PMC5182071},
language = {English},
rating = {0},
date-added = {2017-05-23T15:43:04GMT},
date-modified = {2017-05-23T15:57:00GMT},
abstract = {Detection of recent natural selection is a challenging problem in population genetics. Here we introduce the singleton density score (SDS), a method to infer very recent changes in allele frequencies from contemporary genome sequences. Applied to data from the UK10K Project, SDS reflects allele frequency changes in the ancestors of modern Britons during the past {\textasciitilde}2000 to 3000 years. We see strong signals of selection at lactase and the major histocompatibility complex, and in favor of blond hair and blue eyes. For polygenic adaptation, we find that recent selection for increased height has driven allele frequency shifts across most of the genome. Moreover, we identify shifts associated with other complex traits, suggesting that polygenic adaptation has played a pervasive role in shaping genotypic and phenotypic variation in modern humans.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=27738015&retmode=ref&cmd=prlinks},
